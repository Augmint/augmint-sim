{
    "name": "augmint-sim",
    "version": "0.0.1",
    "description": "Augmint Simulator",
    "homepage": ".",
    "private": true,
    "dependencies": {
        "chart.js": "2.7.1",
        "random-seed": "0.3.0",
        "yarn": "^1.3.2"
    },
    "scripts": {
        "build": "webpack",
        "prod":
            "NODE_ENV=production webpack-dev-server --host 0.0.0.0 --progress --colors --content-base dist/  --inline --config  webpack.config.js",
        "test": "echo tests were invoked but no tests yet. Returning success.",
        "dev":
            "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --progress --colors --content-base dist/  --watch --hot --inline --config  webpack.config.js",
        "dev-win":
            "set NODE_ENV=development & webpack-dev-server --host 0.0.0.0 --progress --colors --content-base dist/  --watch --hot --inline --config  webpack.config.js"
    },
    "devDependencies": {
        "copy-webpack-plugin": "4.4.1",
        "webpack": "3.11.0",
        "webpack-dev-server": "2.11.1"
    }
}
